  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Django",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}\\manage.py",
      "args": ["runserver"],
      "django": true,
      "justMyCode": false
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Chrome",
      "url": "http://localhost:8000",
      "webRoot": "${workspaceFolder}"        
    }
  ],
  "compounds": [{
    "name": "Django/Web",
    "configurations": ["Python: Django", "Chrome"]
    }
  ]
}
